;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	tfg.local. root.tfg.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.tfg.local.
170	IN	PTR	lb.tfg.local.
98	IN	PTR	fw.tfg.local.
